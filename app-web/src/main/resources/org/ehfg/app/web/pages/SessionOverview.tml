<div t:id="layout" xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd" xmlns:p="tapestry:parameter">
	<h1>${message:list.of.sessions}</h1>
	
	<table t:id="sessions" class="table table-striped table-bordered">
		<p:startTimeCell>
			${formattedStartDate}
		</p:startTimeCell>
		
		<p:endTimeCell>
			${formattedEndDate}
		</p:endTimeCell>
		
		<p:detailCell>
			<a t:id="sessionDetails">${message:session.details}</a>
		</p:detailCell>
	</table>
</div>